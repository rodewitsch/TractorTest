{
  "expo": {
    "name": "TractorTest",
    "slug": "TractorTest",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#101010"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "permissions": [],
      "package": "com.rdm.tracktortest",
      "versionCode": 20
    }
  }
}