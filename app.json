{
  "expo": {
    "name": "TractorTest",
    "slug": "TractorTest",
    "version": "3.0.12",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [],
      "package": "com.rdm.tracktortest",
      "versionCode": 31
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
